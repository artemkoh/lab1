n = 5

for i in range(1, n + 1):
    num = 5
    for j in range(n, 0, -1): 
        if j < i:
            print(" ", end=" ")
        else:
            print(num, end=" ")
            num += 0
    print("") 

for i in range(1, n + 1):
    num = 5
    for j in range(i): 
            print(5, end=" ")
    print("") 
