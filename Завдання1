a = int(input("Введіть a (1-100): "))
b = int(input("Введіть b (1-100): "))


if not (1 <= a <= 100 and 1 <= b <= 100):
    print("Помилка! Введені значення мають бути в діапазоні 1-100.")
else:
    if a > b:
        X = b * a + 1
    elif a == b:
        X = 3425
    else:
        X = (2 * a - 5) / b

    print(f"X = {X}")
